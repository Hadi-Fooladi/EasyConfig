<UserControl
	x:ClassModifier="internal"
	x:Class="EasyConfig.Editor.CompoundEditor"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:local="clr-namespace:EasyConfig.Editor"
	mc:Ignorable="d"
	FontSize="16"
	d:DesignWidth="640" d:DesignHeight="480">

	<StackPanel Orientation="Horizontal">
		<DockPanel>
			<Button Name="bNewDel" DockPanel.Dock="Top" Click="bNewDel_OnClick" />
			<ListBox Name="LB" Width="200" SelectionChanged="LB_OnSelectionChanged" />
		</DockPanel>
		<ContentControl Name="FieldEditorContainer" />
	</StackPanel>
</UserControl>
