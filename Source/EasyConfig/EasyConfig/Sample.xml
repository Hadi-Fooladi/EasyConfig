<EasyConfig
	Version="4.2"
	xmlns="EasyConfig"
	xsi:schemaLocation="EasyConfig ../Schema/Schema.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<Enum Name="eBlood" Members="A, B = -2, AB, O = 4" />
	<Enum Name="eGender" Members="Male, Female" />
	<Enum Name="eSeason">
		<Member Name="Spring" Desc="!!!" />
		<Member Name="Summer" Value="1">
			<Desc>
				<Line Value="Line #1" />
				<Line Value="Line #2" />
				<Line Value="Line #3" />
			</Desc>
		</Member>
		<Member Name="Autumn" />
		<Member Name="Winter" Value="-1" />
	</Enum>

	<Enum Name="eComplex" Members="A, B = -2, AB, O = 4">
		<Member Name="Spring"  />
		<Member Name="Summer" Value="1">
			<Desc>
				<Line Value="Line #1" />
				<Line Value="Line #2" />
				<Line Value="Line #3" />
			</Desc>
		</Member>
		<Member Name="Autumn" />
		<Member Name="Winter" Value="-1" />
	</Enum>

	<DataType Name="Complex">
		<Attribute Name="Real" Type="float" />
		<Attribute Name="Imaginary" Type="float" />
	</DataType>

	<DataType Name="Formula2">
		<Field Name="C0" Type="Complex" />
		<Field Name="C1" Type="Complex" />
		<Field Name="C2" Type="Complex" />
	</DataType>

	<DataType Name="ComplexNum" Inherit="Complex">
		<Attribute Name="a" Type="float" />
		<Attribute Name="b" Type="float" />
	</DataType>

	<DataType Name="FormulaN">
		<Field Name="C" Type="Complex" Multiple="Yes" />
	</DataType>

	<DataType Name="Formula3" Inherit="Formula2">
		<Field Name="C3" Type="Complex" />
	</DataType>
	
	<Root Name="Config" Version="1.0" Desc="Config Class">
		<DataType Name="Nested">
			<Attribute Name="b" Type="float" />
			<Field Name="C" Type="Complex" Multiple="Yes" />
		</DataType>

		<Node Name="First">
			<Attribute Name="Num" Type="int" Desc="Amazing stuff" />
			<Attribute Name="Num2" Type="float" />
			<Attribute Name="Num3" Type="float" Default="14" />
			<Attribute Name="Text" Type="string" />
			<Attribute Name="Text2" Type="string" Default='"My Name"' />
			<Attribute Name="Married" Type="yn" />
			<Attribute Name="Single" Type="yn" Default="true" />
			<Attribute Name="C" Type="eComplex" Desc="This is a desc!!!" />
		</Node>

		<Node Name="Second" Desc="Second Class" ReadOnly="No">
			<Node Name="Grade" Multiple="Yes">
				<Attribute Name="Value" Type="float" ReadOnly="Yes" />
				<Attribute Name="Max" Type="float" Default="20" ReadOnly="No" />
			</Node>
			<Node Name="Grade2" Multiple="Yes">
				<Attribute Name="Value" Type="float" />
				<Attribute Name="Max" Type="float" Default="20" />
			</Node>
		</Node>

		<Node Name="Person" TypeName="CPerson">
			<Attribute Name="Age" Type="int" />
			<Attribute Name="Height" Type="float" />
			<Attribute Name="Weight" Type="float" />
			<Attribute Name="Blood" Type="eBlood" />
			<Attribute Name="Gender" Type="eGender" />
			<Attribute Name="SSN" Type="int?" Default="null" />
			<Attribute Name="BirthSeason" Type="eSeason">
				<Desc>
					<Line Value="Line #1" />
					<Line Value="Line #2" />
				</Desc>
			</Attribute>

			<Field Name="UseNested" Type="Nested2" ReadOnly="No" />

			<Field Name="Spouse" Type="CPerson" Desc="Spouse Info" />
			<Field Name="Children" TagName="Child" Type="CPerson" Multiple="Yes" />

			<DataType Name="Nested2" Inherit="Nested">
				<Attribute Name="d" Type="int" />
			</DataType>
		</Node>

		<Node Name="Employee" Inherit="CPerson">
			<Attribute Name="Wage" Type="float">
				<Desc>
					<Line Value="Employee Wage"/>
					<Line Value="?????????????"/>
				</Desc>
			</Attribute>
		</Node>

		<Node Name="Family" TypeName="CFamily">
			<Attribute Name="nChild" Type="int" />
			<Field Name="Husband" Type="CPerson" />
			<Field Name="Wife" Type="CPerson" />
		</Node>

		<Node Name="BigFamily">
			<Field Name="Father" Type="CFamily" />
			<Field Name="Mother" Type="CFamily" />
		</Node>

		<Node Name="Algebra">
			<Field Name="Origin" Type="Complex" />
			<Field Name="Numbers" Type="ComplexNum" Multiple="Yes" />
		</Node>

		<Node Name="ComplexNum2" Inherit="ComplexNum">
			<Attribute Name="c" Type="float" />

			<Node Name="Person">
				<Attribute Name="Age" Type="int" />
				<Attribute Name="Height" Type="float" />
				<Attribute Name="Weight" Type="float" />

				<Field Name="Spouse" Type="PersonData" Desc="Spouse Info" />
				<Field Name="Children" TagName="Child" Type="PersonData" Multiple="Yes" />
			</Node>
		</Node>
	
		<Node Name="MyFormula" Inherit="Formula3">
			<Field Name="C4" Type="Complex">
				<Desc>
					<Line Value="Multi-Line" />
					<Line Value="Comment" />
				</Desc>
			</Field>
		</Node>

		<Node Name="Society">
			<Desc>
				<Line Value="Multiple" />
				<Line Value="Line" />
				<Line Value="!!!!!!!!!!!!!!!!!" />
			</Desc>

			<Node Name="Human" Multiple="Yes">
				<Field Name="C" Type="Complex" Multiple="Yes" />
			</Node>
		</Node>
	</Root>
</EasyConfig>
